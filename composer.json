{
    "name": "lullabot/robo-acquia",
    "description": "A Robo task runner for the Acquia Cloud API.",
    "type": "robo-tasks",
    "require": {
        "typhonius/acquia-php-sdk-v2": "^3.1.0",
        "consolidation/robo": ">=1.0",
        "php": ">=7.1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Sansbury",
            "email": "james@lullabot.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lullabot\\RoboAcquia\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AcquiaCloudApi\\Tests\\": "vendor/typhonius/acquia-php-sdk-v2/tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "php-coveralls/php-coveralls": "^2.1",
        "phpro/grumphp": "^1.16",
        "squizlabs/php_codesniffer": "^3.4",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpmd/phpmd": "^2.7",
        "roave/security-advisories": "dev-master",
        "enlightn/security-checker": "^1.10",
        "maglnet/composer-require-checker": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true
        }
    }
}
